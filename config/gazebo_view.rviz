      Class: rviz/Map
      Draw Behind: false
      Enabled: true
      Name: Map
      Topic: /map
      Value: true
  Global Options:
    Fixed Frame: odom
    Frame Rate: 30
  Name: root
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 10
      Pitch: 0.785398
      Target Frame: base_link
      Value: Orbit (rviz)
      Yaw: 0.785398
